{
  "auto_detect": "yes",
  "customize": {
    "add_to_path": "yes",
    "check_that_exists": "yes",
    "ck_version": 10,
    "env_prefix": "CK_ENV_LIB_CNTK",
    "limit_recursion_dir_search": {
      "linux": 5,
      "win": 5
    },
    "soft_file": {
      "linux": "cntk/__init__.py",
      "win": "cntk\\__init__.py"
    }
  },
  "soft_name": "CNTK library",
  "tags": [
    "lib",
    "cntk"
  ]
}
